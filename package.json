{
  "name": "weather-vue",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mcoypaco/weather-vue.git"
  },
  "bugs": {
    "url": "https://github.com/mcoypaco/weather-vue/issues"
  },
  "homepage": "https://github.com/mcoypaco/weather-vue#readme",
  "release": {
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
