  },
  "dependencies": {
    "@heroicons/vue": "^2.0.13",
    "axios": "^1.2.3",
    "firebase": "^9.15.0",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "pinia": "^2.0.28",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.4.21",
    "semantic-release": "^20.0.2",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
